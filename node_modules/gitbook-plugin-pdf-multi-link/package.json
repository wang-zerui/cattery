{
  "name": "gitbook-plugin-pdf-multi-link",
  "version": "2.1.0",
  "description": "Add a download link for PDF on every page which works with multilingual GitBooks",
  "engines": {
    "gitbook": ">2.5.0"
  },
  "gitbook": {
    "properties": {
      "base": {
        "required": true,
        "title": "Base for the PDF button redirection",
        "type": [
          "string",
          "object"
        ]
      },
      "label": {
        "default": "Download PDF",
        "title": "Label for the PDF button",
        "type": [
          "string",
          "object"
        ]
      }
    }
  },
  "maintainers": [
    {
      "email": "fardeauarmand@gmail.com",
      "name": "armandfardeau"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/armandfardeau/gitbook-plugin-download-pdf-link",
  "dependencies": {
    "badges": "^1.2.4"
  }
}
